object cuentaCorriente {
    var property saldo = 0

    method Depositar(deposito){
        saldo += deposito
    }

    method extraer(extraccion){
        saldo -= extraccion
    }


}

object cuentaConGastos {
    var property saldo = 0

    method Depositar (deposito) {
        saldo += deposito
    }

    method extraer(extraccion) {
        saldo -= extraccion
    }
}



